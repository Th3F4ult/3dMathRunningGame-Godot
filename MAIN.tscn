script/source = "extends CharacterBody3D


const SPEED = 10.0
const JUMP_VELOCITY = 4.5
@export var level = 0
@export var cancontrol = true
var gravity = ProjectSettings.get_setting(\"physics/3d/default_gravity\")

func _physics_process(delta):
	if cancontrol:
		print(get_viewport().get_mouse_position().x)
		if get_viewport().get_mouse_position().x <= 500:
			velocity.x = -10
		else:
			velocity.x = 10
		$Label3D.text = str(level)
		#if not is_on_floor():
			#velocity.y -= gravity * delta
		#if Input.is_action_just_pressed(\"ui_accept\") and is_on_floor():
			#velocity.y = JUMP_VELOCITY # We don't need any of that
		velocity.z = -10

		move_and_slide()
	else:
		velocity = Vector3i(0,0,0)
	$\"../StaticBody3D\".position.z = position.z
	$\"../StaticBody3D2\".position.z = position.z
	if level <= -1 and cancontrol:
		cancontrol = false
		$\"../AnimationPlayer\".play(\"Game_OVER\")
	if Input.is_mouse_button_pressed(MOUSE_BUTTON_LEFT) and not cancontrol:
		get_tree().change_scene_to_file(\"res://MAIN.tscn\")


func _on_area_3d_2_body_exited(body):
	if body.is_in_group(\"player\"):
		$\"../Area3D2\".position.z -= 15
		var instance = preload(\"res://OBSTACLE.tscn\").instantiate()
		instance.position.z = position.z -15
		get_tree().root.get_node(\"Node3D\").add_child(instance)

